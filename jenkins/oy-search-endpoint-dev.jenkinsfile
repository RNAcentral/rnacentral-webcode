pipeline {
    agent any
    stages {
        stage("switch text search endpoint to dev on OY") {
            environment {
                USERPASSWORD = credentials('refresh_vdbs_login')
                USER = "${env.USERPASSWORD_USR}"
                PASSWORD = "${env.USERPASSWORD_PSW}"
            }
            steps {
                sh '''
                    cd rnacentral
                    fab ebi_cli --password=$PASSWORD refresh_dev
                '''
            }
        }
    }
}